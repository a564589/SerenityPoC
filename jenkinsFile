node {

	stage('SCM Checkout') {
	
		git 'https://github.com/a564589/SerenityPoC'
	
	}
	
	stage('Compile-Package') {
	
		def mavenHome = tool name: 'Maven', type: 'maven'
		script '${mavenHome}/bin/mvn clean verify -Dcucumber.options="--tags @Tag" serenity:aggregate serenity:reports -Dserenity.reports=email'
	
	}
	
}